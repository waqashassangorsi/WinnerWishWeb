<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Oxygenna Typography Module</title>
    <parser>
        <target>docs</target>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <files>
        <directory>inc</directory>
        <ignore>*/vendor/*</ignore>
    </files>
</phpdoc>